{
  "openapi": "3.1.0",
  "info": {
    "title": "Market Pulse API",
    "description": "Job market intelligence API providing tech job listings and salary benchmarks. Search jobs from The Muse and Adzuna by title, company, location, category, or level. Compare compensation across cities, experience levels, and companies.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://market-pulse-app.25raqj64rgw0.us-south.codeengine.appdomain.cloud"
    }
  ],
  "paths": {
    "/jobs/search": {
      "get": {
        "tags": [
          "Jobs"
        ],
        "summary": "Search job listings by title, company, location, category, or level",
        "description": "Search tech job listings from The Muse and Adzuna. All text filters are case-insensitive partial matches. Combine multiple filters to narrow results. Use the 'source' parameter to filter by data source ('themuse' or 'adzuna'). Adzuna jobs include salary data visible in the detail endpoint. The 'level' filter only applies to Muse jobs.",
        "operationId": "search_jobs",
        "parameters": [
          {
            "name": "title",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Job title keyword, e.g. 'Data Scientist'",
              "title": "Title"
            },
            "description": "Job title keyword, e.g. 'Data Scientist'"
          },
          {
            "name": "company",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Company name, e.g. 'Google'",
              "title": "Company"
            },
            "description": "Company name, e.g. 'Google'"
          },
          {
            "name": "location",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "City or region, e.g. 'New York'",
              "title": "Location"
            },
            "description": "City or region, e.g. 'New York'"
          },
          {
            "name": "category",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Job category, e.g. 'Software Engineering', 'Data Science', 'IT Jobs'",
              "title": "Category"
            },
            "description": "Job category, e.g. 'Software Engineering', 'Data Science', 'IT Jobs'"
          },
          {
            "name": "level",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Seniority level: 'Entry Level', 'Mid Level', or 'Senior Level' (Muse jobs only)",
              "title": "Level"
            },
            "description": "Seniority level: 'Entry Level', 'Mid Level', or 'Senior Level' (Muse jobs only)"
          },
          {
            "name": "source",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Filter by data source: 'themuse', 'adzuna', or omit for both",
              "title": "Source"
            },
            "description": "Filter by data source: 'themuse', 'adzuna', or omit for both"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1,
              "description": "Max results to return",
              "default": 25,
              "title": "Limit"
            },
            "description": "Max results to return"
          },
          {
            "name": "skip",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 0,
              "description": "Number of results to skip for pagination",
              "default": 0,
              "title": "Skip"
            },
            "description": "Number of results to skip for pagination"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobSearchResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
