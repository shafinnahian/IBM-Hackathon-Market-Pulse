    "/jobs/filters": {
      "get": {
        "tags": [
          "Jobs"
        ],
        "summary": "Look up valid filter values for a specific field",
        "description": "Returns matching values for a single filterable field (locations, categories, or levels). Use the optional 'q' parameter to search within the field. For locations, searching by state name (e.g. 'california') also returns entries using the abbreviation ('CA') and vice versa. Use this tool when you need to discover valid values for job search filters.",
        "operationId": "get_job_filters",
        "parameters": [
          {
            "name": "field",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "description": "Field to look up: 'locations', 'categories', or 'levels'",
              "title": "Field"
            },
            "description": "Field to look up: 'locations', 'categories', or 'levels'"
          },
          {
            "name": "q",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "description": "Optional search term to filter results, e.g. 'california' or 'software'",
              "title": "Q"
            },
            "description": "Optional search term to filter results, e.g. 'california' or 'software'"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 50,
              "minimum": 1,
              "description": "Max values to return (default 20)",
              "default": 20,
              "title": "Limit"
            },
            "description": "Max values to return (default 20)"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobFiltersResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
